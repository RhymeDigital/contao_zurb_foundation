{
    "name": "rhymedigital/contao_zurb_foundation",
    "description": "A set of tools for Contao Open Source CMS to utilize the Foundation responsive framework",
    "keywords": [
    	"zurb", 
    	"foundation"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "http://rhyme.digital"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core-bundle": "~3.5 || ~4.1",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0",
        "codefog/contao-haste": ">=4.0.0,<5.0-dev",
        "menatwork/contao-multicolumnwizard": ">=3.2.0,<3.3-dev",
        "zurb/foundation": ">=5.5.2,<5.6-dev",
        "jalle19/php-yui-compressor": ">=dev-master",
        "tedivm/jshrink": ">=1.0.0,<1.1-dev",
        "components/jquery": ">=2.1.0,<2.2-dev",
        "components/modernizr": ">=2.6.2,<2.7-dev"
    },
    "autoload": {
        "psr-0": {
            "Rhyme\\": [
                "library/"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/zurb_foundation"
            }
        }
    },
    "replace": {
        "hb-agency/contao_zurb_foundation":"self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}