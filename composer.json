{
    "name": "rhymedigital/contao_zurb_foundation",
    "description": "A set of tools for Contao Open Source CMS to utilize the Foundation responsive framework",
    "keywords": [
        "zurb",
        "foundation"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "http://rhyme.digital"
        }
    ],
    "require": {
        "php":">=5.4",
        "contao/core-bundle": "^4.3",
        "codefog/contao-haste": "^4.0",
        "menatwork/contao-multicolumnwizard": "^3.3",
        "zurb/foundation": "^6.3",
        "jalle19/php-yui-compressor": "^1.0",
        "tedivm/jshrink": "^1.1",
        "components/jquery": "^3.2",
        "components/modernizr": "^2.7"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "Rhyme\\ContaoZurbFoundationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        },
        "contao-manager-plugin": "Rhyme\\ContaoZurbFoundationBundle\\ContaoManager\\Plugin"
    },
    "replace": {
        "hb-agency/contao_zurb_foundation":"self.version"
    }
}